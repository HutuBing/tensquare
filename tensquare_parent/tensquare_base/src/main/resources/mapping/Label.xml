<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.tensquare.base.entity.Label">
        <table name="tb_label" schema="tensquare" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" length="20"/>
            </id>
            <basic name="labelName">
                <column name="label_name" nullable="false" length="100"/>
            </basic>
            <basic name="state">
                <column name="state" nullable="false" length="1"/>
            </basic>
            <basic name="count">
                <column name="count" nullable="false" precision="20"/>
            </basic>
            <basic name="recommend">
                <column name="recommend" nullable="false" length="1"/>
            </basic>
            <basic name="fans">
                <column name="fans" nullable="false" precision="20"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
